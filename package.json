{
  "name": "create-banana",
  "version": "2.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/TMname1/create-banana.git"
  },
  "type": "module",
  "description": "TMname1's CLI tool to create customized templates",
  "main": "index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "cz",
    "prepare": "husky install",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli .",
    "lint-staged": "lint-staged",
    "build": "tsc --noEmit && node ./script/build.js"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  },
  "keywords": [
    "CLI",
    "template",
    "generator"
  ],
  "author": "TMname1",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "ejs": "^3.1.10",
    "figlet": "^1.9.4",
    "fs-extra": "^11.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
